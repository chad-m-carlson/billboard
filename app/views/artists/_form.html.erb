<h1><%= @artist.id ? 'Edit artist' : 'Create Artist' %></h1>
<%= form_for @artist do |f| %>

  <%= f.label :artist_name, "Name" %>
  <%= f.text_field :artist_name %>
  <br />
  <br />
  <br />
  <br />
  <%= f.label :genre, "Genre" %>
  <%= f.text_field :genre %>
  <br />
  <br />
  <br />
  <br />
  <%= f.submit %>

<% end %>